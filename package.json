{
  "name": "replaceplastic",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:staging": "vue-cli-service build --mode staging",
    "lint": "vue-cli-service lint",
    "build-ios": "npx cross-env BUILD_OS=ios npm run build && cordova build --release ios",
    "build-android": "npx cross-env BUILD_OS=android npm run build && cordova build --release android",
    "build-web": "npx cross-env BUILD_OS=web npm run build",
    "release-android": "./release-android.sh",
    "resources": "npx cordova-splash --config=config.xml --splash=./resources/splash.png && npx cordova-icon --config=config.xml --icon=./resources/icon.png"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "barcoder": "^2.0.1",
    "chart.js": "^2.7.2",
    "cordova-android": "^7.1.1",
    "cordova-ios": "^4.5.5",
    "cordova-plugin-android-permissions": "^1.0.0",
    "cordova-plugin-ios-camera-permissions": "^1.2.0",
    "cordova-plugin-launchscreen-storyboard": "^2.0.0",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-plugin-x-socialsharing": "^5.4.1",
    "countup.js": "^1.9.3",
    "date-fns": "^1.29.0",
    "es6-promise-plugin": "^4.2.2",
    "phonegap-plugin-barcodescanner": "^8.0.0",
    "quagga": "^0.12.1",
    "register-service-worker": "^1.0.0",
    "typeface-lato": "0.0.54",
    "typeface-slabo-13px": "0.0.54",
    "typeface-slabo-27px": "0.0.54",
    "vue": "^2.5.17",
    "vue-carousel": "^0.10.0",
    "vue-chartjs": "^3.4.0",
    "vue-countup-v2": "^1.0.3",
    "vue-localstorage": "^0.6.2",
    "vue-mq": "^0.2.1",
    "vue-router": "^3.0.1",
    "vue-scrollto": "^2.11.0",
    "vue-spinner": "^1.0.3",
    "vue2-animate": "^2.0.0",
    "vuewordcloud": "^18.7.11",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "cordova-plugin-android-permissions": "^1.0.0",
    "phonegap-plugin-barcodescanner": "^8.0.0",
    "cordova-plugin-ios-camera-permissions": "^1.2.0",
    "cordova-plugin-launchscreen-storyboard": "^2.0.0",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-plugin-x-socialsharing": "^5.4.1",
    "@vue/cli-plugin-babel": "^3.0.0",
    "@vue/cli-plugin-eslint": "^3.0.0",
    "@vue/cli-plugin-pwa": "^3.0.0",
    "@vue/cli-service": "^3.0.0",
    "@vue/eslint-config-prettier": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "cordova-icon": "^1.0.0",
    "cordova-splash": "^1.0.0",
    "cross-env": "^5.2.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "style-resources-loader": "^1.2.1",
    "vue-template-compiler": "^2.5.17"
  },
  "cordova": {
    "plugins": {
      "cordova-plugin-launchscreen-storyboard": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-whitelist": {},
      "phonegap-plugin-barcodescanner": {
        "ANDROID_SUPPORT_V4_VERSION": "24.+"
      },
      "cordova-plugin-android-permissions": {},
      "cordova-plugin-ios-camera-permissions": {
        "CAMERA_USAGE_DESCRIPTION": "This app needs camera access"
      },
      "cordova-plugin-x-socialsharing": {}
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}